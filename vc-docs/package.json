{
  "name": "@vc-view/vc-docs",
  "version": "1.0.0",
  "description": "vc-docs",
  "main": "./dist/vc-docs.umd.js",
  "module": "./dist/vc-docs.es.js",
  "exports": {
    ".": {
      "import": "./dist/vc-docs.es.js",
      "require": "./dist/vc-docs.umd.js"
    }
  },
  "types": "dist/src/index.d.ts",
  "author": "JobinJia",
  "license": "MIT",
  "scripts": {
    "dev": "vitepress-fc dev --root=docs --host --port 8090 --open",
    "build": "vite build && yarn type",
    "docs-build": "rimraf docs/dist && cross-env NODE_ENV=production vitepress-fc build --root=docs",
    "docs-serve": "cross-env NODE_ENV=production vitepress-fc serve --root=docs",
    "docs-build-serve": "cross-env NODE_ENV=true yarn docs-build && yarn docs-serve",
    "docs-deploy": "gh-pages -d docs/dist -t true",
    "docs-build-deploy": "yarn docs-build && yarn docs-deploy"
  },
  "dependencies": {
    "@vc-view/vc-naive": "workspace:^1.0.0"
  },
  "devDependencies": {
    "gh-pages": "^3.1.0",
    "vitepress-for-component": "^0.16.6"
  },
  "keywords": [
    "vue",
    "vue3",
    "components"
  ],
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  }
}
