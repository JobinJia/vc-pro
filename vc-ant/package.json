{
  "name": "@vc-pro/vc-ant",
  "version": "1.0.0",
  "description": "",
  "main": "./dist/vc-ant.js",
  "types": "./dist/types/components/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/vc-ant.js"
    },
    "./dist/style.css": {
      "import": "./dist/style.css"
    },
    "./dist/": {
      "import": "./dist/es/components/"
    }
  },
  "files": [
    "dist",
    "README.md",
    "package.json",
    "LICENSE"
  ],
  "scripts": {
    "tsc:ant": "vue-tsc --noEmit",
    "clean:ant": "rimraf dist",
    "build:ant": "pnpm run clean:ant && pnpm run build:ant-all && pnpm run build:ant-esm",
    "build:ant-all": "vite build --config vite.ant.ts",
    "build:ant-esm": "vite build --config vite.ant.esm.ts",
    "publish:vc-ant": "npm publish --access public",
    "unpublish:vc-ant": "npm unpublish vc-ant --force"
  },
  "peerDependencies": {
    "ant-design-vue": "^3.0.0-beta.5",
    "lodash-es": "^4.17.21",
    "vue": "^3.2.26"
  },
  "keywords": [],
  "author": "",
  "license": "MIT"
}
