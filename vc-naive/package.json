{
  "name": "@vc-view/vc-naive",
  "version": "0.0.98",
  "description": "",
  "main": "./dist/vc-naive.js",
  "types": "./es/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/vc-naive.js",
      "types": "./index.d.ts"
    },
    "./dist/style.css": {
      "import": "./dist/style.css"
    },
    "./*": "./*"
  },
  "files": [
    "dist",
    "es",
    "README.md",
    "package.json",
    "LICENSE"
  ],
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build:naive": "pnpm run clean:naive && pnpm run build:naive-all && pnpm run build:naive-esm",
    "build:naive-all": "vite build --config vite.naive.ts",
    "build:naive-esm": "vite build --config vite.naive.esm.ts",
    "clean:naive": "rimraf dist && rimraf es",
    "publish:vc-naive": "npm publish --access public",
    "unpublish:vc-naive": "npm unpublish vc-view --force",
    "tsc:naive": "vue-tsc --noEmit"
  },
  "peerDependencies": {
    "vue": "^3.2.26",
    "lodash-es": "^4.17.21",
    "naive-ui": "^2.23.2"
  },
  "keywords": [],
  "author": "",
  "license": "ISC"
}
