{
  "name": "vc-pro/monorepo",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "lint:eslint": "eslint --cache --max-warnings 0  \"./**/*.{vue,ts,tsx}\" --fix",
    "lint:prettier": "prettier --write  \"./**/*.{js,ts,json,tsx,css,less,scss,vue,html,md}\"",
    "patch-pkg-version": "cd script && zx ./patch-version.mjs",
    "get-pkg-version": "cd script && zx ./get-version.js",
    "tsc:naive": "pnpm -C ./vc-naive tsc:naive",
    "docs:dev": "vitepress-fc dev docs --port 8090 --open --host",
    "docs:build": "vitepress-fc build docs",
    "docs:serve": "vitepress-fc serve",
    "build:vc-naive": "pnpm -C ./vc-naive build:naive",
    "build:vc-ant": "pnpm -C ./vc-ant build:ant",
    "build:vc-share": "pnpm -C ./shared build:shared",
    "build:vc-composables": "pnpm -C ./vc-composables build:hooks",
    "build:all": "pnpm run build:vc-naive && pnpm run build:vc-share && pnpm run build:vc-composables && pnpm run build:vc-ant",
    "publish:shared": "pnpm -C ./shared publish:shared",
    "publish:vc-naive": "pnpm -C ./vc-naive publish:vc-naive",
    "publish:vc-ant": "pnpm -C ./vc-ant publish:vc-ant",
    "publish:vc-composables": "pnpm -C ./vc-composables publish:vc-composables",
    "publish": "pnpm run publish:shared && pnpm run publish:vc-naive && pnpm run publish:vc-composables && pnpm run publish:vc-ant"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/lodash": "^4.14.178",
    "@types/node": "^17.0.12",
    "@vc-pro/shared": "workspace:*",
    "@vc-pro/vc-ant": "workspace:*",
    "@vc-pro/vc-composables": "workspace:*",
    "@vc-pro/vc-naive": "workspace:*",
    "@vitejs/plugin-vue": "^2.1.0",
    "@vitejs/plugin-vue-jsx": "^1.3.3",
    "@vue/compiler-sfc": "^3.2.29",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^10.0.0",
    "ant-design-vue": "^3.0.0-beta.8",
    "element-plus": "^1.2.0-beta.6",
    "eslint": "^8.7.0",
    "eslint-define-config": "^1.2.3",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^8.3.0",
    "fs-extra": "^10.0.0",
    "less": "^4.1.2",
    "lodash-es": "^4.17.21",
    "naive-ui": "^2.24.6",
    "prettier": "^2.5.1",
    "react": "16.8.0",
    "react-dom": "16.8.0",
    "rimraf": "^3.0.2",
    "tsup": "^5.11.11",
    "typescript": "^4.5.5",
    "vite": "^2.7.13",
    "vite-plugin-dts": "^0.9.9",
    "vitepress-for-component": "^0.16.9",
    "vue": "^3.2.29",
    "vue-tsc": "^0.31.1",
    "zx": "^4.3.0"
  }
}
